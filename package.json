{
  "name": "boilerplate-nodejs-loopback",
  "version": "1.1.0",
  "description": "boilerplate-nodejs-loopback",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^18.17.0 || >=20.5.0"
  },
  "scripts": {
    "build": "lb-tsc -p tsconfig.live.json",
    "build:dev": "lb-tsc -p tsconfig.json",
    "rebuild": "yarn run clean && yarn run build",
    "rebuild:dev": "yarn run clean && yarn run build:dev",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "migrate": "yarn run migrate:latest",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "start": "node -r source-map-support/register .",
    "start:dev": "nodemon -r source-map-support/register .",
    "prestart": "yarn run rebuild && yarn run migrate",
    "prestart:dev": "yarn run rebuild:dev && yarn run migrate",
    "test": "./run-tests.sh",
    "posttest": "yarn run lint:fix && yarn run lint && yarn run coverage",
    "coverage": "open coverage/index.html",
    "coverage:ci": "lb-nyc report --reporter=text-lcov | coveralls",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "openapi-spec": "node ./dist/openapi-spec",
    "preopenapi-spec": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Alex Pop <alexandru.pop@sfappworks.com>",
  "license": "UNLICENSED",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@babel/node": "7.22.19",
    "@loopback/authentication": "10.1.3",
    "@loopback/authentication-jwt": "0.14.3",
    "@loopback/authentication-passport": "6.1.3",
    "@loopback/authorization": "0.14.3",
    "@loopback/boot": "6.1.3",
    "@loopback/build": "10.1.3",
    "@loopback/core": "5.1.3",
    "@loopback/eslint-config": "14.0.4",
    "@loopback/logging": "0.11.4",
    "@loopback/repository": "6.1.3",
    "@loopback/rest": "13.1.3",
    "@loopback/rest-explorer": "6.1.3",
    "@loopback/security": "0.10.3",
    "@loopback/service-proxy": "6.1.3",
    "bcryptjs": "2.4.3",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "jsonwebtoken": "9.0.2",
    "knex": "3.0.1",
    "loopback-connector-postgresql": "6.0.8",
    "postgres": "3.4.1",
    "socket.io": "4.7.2",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@loopback/testlab": "6.1.3",
    "@types/bcryptjs": "2.4.4",
    "@types/jsonwebtoken": "9.0.3",
    "@types/node": "16.18.55",
    "@types/uuid": "9.0.6",
    "eslint": "8.50.0",
    "nodemon": "3.0.1",
    "socket.io-client": "4.7.2",
    "source-map-support": "0.5.21"
  }
}
